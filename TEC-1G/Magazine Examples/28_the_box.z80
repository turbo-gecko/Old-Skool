; Move a Box>>

	ORG	4000H
START:
	LD	IX,TABLE
	LD	B,00H
	LD	D,1CH
LOOP1:
	LD	C,40H
LOOP2:
	LD	A,(IX+0)
	OUT	(01),A
	LD	A,(IX+1)
	OUT	(02),A
LOOP3:
	DJNZ	LOOP3

	XOR	A
	OUT	(02),A
	LD	A,(IX+2)
	OUT	(01),A
	LD	A,(IX+3)
	OUT	(02),A
